{
  "schemaVersion": 1,
  "id": "pixelmon",
  "version": "${version}",
  "name": "OpenPixelmon",
  "description": "An rewrite of pixelmon reforged & generations mod for fabric",
  "authors": [
    "OpenPixelmon Contributors"
  ],
  "contact": {
    "repo": "https://github.com/MartyAlana/OpenPixelmon"
  },
  "license": "MIT",
  "icon": "assets/open_pixelmon/textures/item/poke_ball.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.marty.openpixelmon.client.OpenPixelmonClient"
    ],
    "main": [
      "me.marty.openpixelmon.OpenPixelmon"
    ],
    "cardinal-components-entity": [
      "me.marty.openpixelmon.api.component.EntityComponents"
    ]
  },
  "accessWidener": "pixelmon.aw",
  "mixins": [
    "pixelmon.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.10.8",
    "fabric": ">=0.29.0",
    "minecraft": "20w51a"
  },
  "custom": {
    "cardinal-components": [
      "open_pixelmon:pixelmon_party"
    ]
  }
}
